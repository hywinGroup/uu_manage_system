
.loading-bg{
	position:fixed;
	height:100%;
	width:100%;
	top:0;
	left:0;
}

@keyframes spin-loading
{
  from {transform: rotate(0deg)}
  to {transform: rotate(360deg)}
}
@-webkit-keyframes spin-loading 
{
  from {transform: rotate(0deg)}
  to {transform: rotate(360deg)}
}
.loading-img{
  width:80rpx;
  height:80rpx;
  background: url(data:image/png;base64,) 0 0 no-repeat;
  background-size:80rpx;
  -webkit-transform-origin: 50% 49%;
  -moz-transform-origin: 50% 49%;
  -ms-transform-origin: 50% 49%;
  transform-origin: 50% 49%;
  position: absolute;
  left: 50%;
  top: 50%;
  margin-left: -40rpx;
  -webkit-animation: spin-loading .5s linear infinite;
  -moz-animation: spin-loading .5s linear infinite;
  -ms-animation: spin-loading .5s linear infinite;
  animation: spin-loading .5s linear infinite;
}